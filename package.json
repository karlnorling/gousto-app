{
  "name": "gousto-app",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.ts",
  "dependencies": {
    "csvtojson": "^2.0.8",
    "koa": "^2.7.0",
    "koa-bodyparser": "^2.5.0",
    "koa-helmet": "^4.2.0",
    "koa-router": "^7.4.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@koa/cors": "^2.2.3",
    "@types/csvtojson": "^1.1.5",
    "@types/jest": "^24.0.13",
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.40",
    "@types/node": "^12.0.4",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2"
  },
  "scripts": {
    "test": "jest --watchAll",
    "start": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/server/index.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "engines": {
    "node": "12.x.x",
    "npm": "6.x.x"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
